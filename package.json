{
  "name": "w2l",
  "version": "0.1.0",
  "description": "An imperative library for LLM-driven structured image generation",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:docs": "typedoc",
    "dev": "tsc -p tsconfig.build.json --watch",
    "dev:docs": "typedoc --watch",
    "dev:playground": "cd playground && npm run dev",
    "dev:all": "concurrently -n \"docs,playground\" -c \"green,yellow\" \"npm run dev:docs\" \"npm run dev:playground\"",
    "prepare": "npm run build",
    "playground": "cd playground && npm run dev"
  },
  "keywords": [
    "svg",
    "graphics",
    "visual",
    "layout",
    "llm",
    "image-generation"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.3.0"
  }
}
