{
  "name": "w2l",
  "version": "0.1.0",
  "description": "An imperative library for LLM-driven structured image generation",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json && node scripts/generate-playground-types.js",
    "build:docs": "typedoc",
    "dev": "tsc -p tsconfig.build.json --watch",
    "dev:docs": "typedoc --watch",
    "dev:playground": "cd playground && npm run dev",
    "dev:lib": "concurrently -n \"tsc,types\" -c \"blue,cyan\" \"tsc -p tsconfig.build.json --watch\" \"node scripts/watch-and-generate-types.js\"",
    "dev:all": "concurrently -n \"lib,docs,playground\" -c \"blue,green,yellow\" \"npm run dev:lib\" \"npm run dev:docs\" \"npm run dev:playground\"",
    "prepare": "npm run build",
    "playground": "cd playground && npm run dev"
  },
  "keywords": [
    "svg",
    "graphics",
    "visual",
    "layout",
    "llm",
    "image-generation"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.0",
    "typedoc": "^0.27.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "csstype": "^3.1.3"
  }
}
