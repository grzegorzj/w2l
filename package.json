{
  "name": "w2l",
  "version": "0.1.0",
  "description": "An imperative library for LLM-driven structured image generation",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json && node scripts/generate-playground-types.js && npm test -- --no-interactive",
    "build:docs": "typedoc",
    "dev": "tsc -p tsconfig.build.json --watch",
    "dev:docs": "typedoc --watch",
    "dev:playground": "cd playground && npm run dev",
    "dev:server": "cd server && npm run dev",
    "dev:lib": "concurrently -n \"tsc,types,tests\" -c \"blue,cyan,green\" \"tsc -p tsconfig.build.json --watch\" \"node scripts/watch-and-generate-types.js\"",
    "dev:all": "concurrently -n \"lib,docs,server,indexer,playground\" -c \"blue,green,magenta,cyan,yellow\" \"npm run dev:lib\" \"npm run dev:docs\" \"npm run dev:server\" \"npm run dev:playground\"",
    "dev:llm-playground": "concurrently -n \"server,indexer,playground\" -c \"magenta,cyan,yellow\" \"npm run dev:server\"  \"npm run dev:playground\"",
    "prepare": "npm run build",
    "playground": "cd playground && npm run dev",
    "server": "cd server && npm run dev",
    "test": "node tests/snapshot-runner.js",
    "test:watch": "node tests/snapshot-runner.js --watch",
    "test:ci": "npm test -- --no-interactive"
  },
  "keywords": [
    "svg",
    "graphics",
    "visual",
    "layout",
    "llm",
    "image-generation"
  ],
  "author": "",
  "devDependencies": {
    "concurrently": "^8.2.0",
    "typedoc": "^0.27.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "csstype": "^3.1.3",
    "dotenv": "^17.2.3",
    "express": "^5.1.0"
  }
}
